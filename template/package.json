{
  "name": "{{projectName}}",
  "version": "1.0.0",
  "description": "My Moleculer project",
  "scripts": {
    "dev": "moleculer-runner --repl services",
    "start": "moleculer-runner services"{{#jest}},
    "ci": "jest --watch",
    "test": "jest --coverage"{{/jest}}{{#lint}},
    "lint": "eslint services"{{/lint}}
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "author": "",
  "devDependencies": {
    {{#lint}}
    "eslint": "4.3.0",
    {{/lint}}
    {{#jest}}
    "jest": "20.0.4",
    "jest-cli": "20.0.4",
    {{/jest}}
    "moleculer-repl": "^0.1.3"
  },
  "dependencies": {
    {{#apiGW}}
    "moleculer-web": "^0.4.1",
    {{/apiGW}}
    {{#if_eq transporter "NATS"}}
    "nats": "0.7.20",
    {{/if_eq}}
    {{#if_eq transporter "MQTT"}}
    "mqtt": "2.9.0",
    {{/if_eq}}
    {{#if_eq transporter "Redis"}}
    "ioredis": "3.1.1",
    {{/if_eq}}
    {{#if_eq cacher "Redis"}}
    "ioredis": "3.1.1",
    {{/if_eq}}
    "moleculer": "^0.8.4"
  },
  "engines": {
    "node": ">= 6.x.x"
  }{{#jest}},
  "jest": {
    "testEnvironment": "node",
    "rootDir": "./services",
    "roots": [
      "../test"
    ]
  }{{/jest}}
}
